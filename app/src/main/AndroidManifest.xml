<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.radenmas.peminjamanperalatan">

    <uses-sdk tools:overrideLibrary="com.google.zxing.client.android" />

    <uses-permission android:name="android.permission.INTERNET " />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:installLocation="preferExternal"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PeminjamanPeralatan">
        <activity android:name=".AdminRegistrasi" />
        <activity android:name=".admin.AdminAsistenLab" />
        <activity android:name=".admin.AdminPreviewDataMhs" />
        <activity android:name=".admin.AdminAccount" />
        <activity android:name=".admin.AdminDetailBioMhs" />
        <activity android:name=".admin.AdminDetailPinjaman" />
        <activity android:name=".admin.AdminKelas" />
        <activity
            android:name=".user.Capture"
            android:screenOrientation="portrait"
            android:stateNotNeeded="true"
            android:theme="@style/zxing_CaptureTheme" />
        <activity android:name=".About" />
        <activity android:name=".admin.AdminDataPeminjam" />
        <activity android:name=".admin.AdminLab" />
        <activity android:name=".admin.AdminBioMhs" />
        <activity android:name=".admin.AdminMainActivity" />
        <activity android:name=".user.UserMainActivity" />
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity
            android:name=".Splash"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>